{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "apps",
	"projects": {
		"modern-mfe-nx-ng-ui": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "apps/modern-mfe-nx-ng-ui",
			"sourceRoot": "apps/modern-mfe-nx-ng-ui/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/modern-mfe-nx-ng-ui",
						"index": "apps/modern-mfe-nx-ng-ui/src/index.html",
						"main": "apps/modern-mfe-nx-ng-ui/src/main.ts",
						"polyfills": "apps/modern-mfe-nx-ng-ui/src/polyfills.ts",
						"tsConfig": "apps/modern-mfe-nx-ng-ui/tsconfig.app.json",
						"assets": [
							"apps/modern-mfe-nx-ng-ui/src/favicon.ico",
							"apps/modern-mfe-nx-ng-ui/src/assets"
						],
						"styles": [
							"apps/modern-mfe-nx-ng-ui/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "apps/modern-mfe-nx-ng-ui/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/modern-mfe-nx-ng-ui/src/environments/environment.ts",
									"with": "apps/modern-mfe-nx-ng-ui/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "apps/modern-mfe-nx-ng-ui/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "modern-mfe-nx-ng-ui:build:production",
							"extraWebpackConfig": "apps/modern-mfe-nx-ng-ui/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "modern-mfe-nx-ng-ui:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"publicHost": "http://localhost:5000",
						"port": 5000,
						"extraWebpackConfig": "apps/modern-mfe-nx-ng-ui/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "modern-mfe-nx-ng-ui:build",
						"extraWebpackConfig": "apps/modern-mfe-nx-ng-ui/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "apps/modern-mfe-nx-ng-ui/src/test.ts",
						"polyfills": "apps/modern-mfe-nx-ng-ui/src/polyfills.ts",
						"tsConfig": "apps/modern-mfe-nx-ng-ui/tsconfig.spec.json",
						"karmaConfig": "apps/modern-mfe-nx-ng-ui/karma.conf.js",
						"assets": [
							"apps/modern-mfe-nx-ng-ui/src/favicon.ico",
							"apps/modern-mfe-nx-ng-ui/src/assets"
						],
						"styles": [
							"apps/modern-mfe-nx-ng-ui/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
